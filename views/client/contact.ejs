<!-- Contact Us Start -->
<section class="page-contact-us">
	<div class="container">
		<div class="row align-items-center">
			<div class="col-lg-6 order-lg-1 order-2">
				<div class="contact-us-content wow left-animation" data-wow-duration="0.8s"
					data-wow-delay="0.2s">
					<span class="sub-title">CONTACT US</span>
					<h2 class="h2-title">Get In Touch Now</h2>
					<div class="contact-link-list">
						<div class="contact-link-box">
							<div class="icon">
								<img src="../assets/images/location-icon.svg" width="35" height="40" alt="Location">
							</div>
							<div class="text">
								<h4 class="h4-title">Location</h4>
								<p>
									<a href="https://maps.app.goo.gl/nZAczLhHmdtbNUNj6" target="_blank" title="123 Demo Street, Kingston, Jamaica">123 Demo Street, Kingston, Jamaica</a>
								</p>
							</div>
						</div>
						<div class="contact-link-box">
							<div class="icon">
								<img src="../assets/images/email-icon.svg" width="38" height="28" alt="Email Address">
							</div>
							<div class="text">
								<h4 class="h4-title">Email Address</h4>
								<p>
									<a href="mailto:support@elaw.com" title="Mail to support@elaw.com">
										<span class="__cf_email__">support@elaw.com</span>
									</a>
								</p>
							</div>
						</div>
						<div class="contact-link-box">
							<div class="icon">
								<img src="../assets/images/phone-icon.svg" width="35" height="35" alt="Phone No.">
							</div>
							<div class="text">
								<h4 class="h4-title">Phone No.</h4>
								<p>
									<a href="tel:8761234567" title="Call on +1 876 123 4567">+1 876 123 4567</a>
								</p>
							</div>
						</div>
					</div>
				</div>
			</div>
	

			
			<div class="col-lg-6 order-lg-2 order-1">
				<div class="sidebar wow right-animation" data-wow-duration="0.8s" data-wow-delay="0.2s">
					<div class="get-in-touch">
						<h4 class="h4-title">Get In Touch</h4>
						<div class="get-in-touch-form">
							<form method="post">
								<div class="form-box">
									<input type="text" class="form-input" id="name" name="name" placeholder="Full Name*" autocomplete="off" required>
								</div>
								<div class="form-box">
									<input type="email" class="form-input" id="email" name="email" placeholder="Email Address*" autocomplete="off" required>
								</div>
								<div class="form-box">
									<textarea class="form-input" id="message" name="message"  placeholder="Message..." required></textarea>
								</div>
								<div class="form-box">
									<button type="submit" class="sec-btn" name="submit"><span>Submit Now</span></button>
								</div>
							</form>
						</div>
					</div>

				</div>
			</div>


		</div>
	</div>
</section>
<!-- Contact Us End -->

