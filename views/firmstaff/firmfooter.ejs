<script>
  document.addEventListener('DOMContentLoaded', () => {
  const trnInputs = document.querySelectorAll('[data-kil-trn1]');

  trnInputs.forEach(input => {
    input.addEventListener('input', () => {
      // Remove all non-numeric characters
      let value = input.value.replace(/\D/g, '');
      if (value.length > 3 && value.length <= 6) {
        input.value = value.slice(0, 3) + '-' + value.slice(3);
      } else if (value.length > 6) {
        input.value = value.slice(0, 3) + '-' + value.slice(3, 6) + '-' + value.slice(6, 9);
      } else {
        input.value = value;
      }
    });
  });
});

  document.addEventListener('DOMContentLoaded', () => {
  const trnInputs = document.querySelectorAll('[data-kil-trn]');

  trnInputs.forEach(input => {
    input.addEventListener('input', () => {
      let value = input.value.slice(0, 9);
      input.value = value;
    });
  });
});
</script>






